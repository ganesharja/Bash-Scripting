pipeline {
    agent any
    environment{
        ENV_URL = "pipleline.google.com"
    }
    stages {
        stage ("Stage one"){
            steps {
            sh '''
                echo "hello world"
                echo "This is stage one"
                echo "i am ganesh arja"
                '''
            }
            }
        stage ("Stage two"){
            steps {
            sh '''
                echo "hello world"
                echo "This is stage Two"
                echo "i am ganesh arja"
                '''
            }
        }
        stage ("Stage Three"){
            steps {
            sh '''
                echo "hello world"
                echo "This is stage Three"
                echo "i am ganesh arja"
                '''
            }
        }
     } 
    
}